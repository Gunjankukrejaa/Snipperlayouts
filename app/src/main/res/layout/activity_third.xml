<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_4"
    tools:context=".ThirdActivity">

    <TextView
        android:id="@+id/textview_18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="32dp"
        android:background="@color/white"
        android:text="Hobbies"
        android:textColor="@color/black"
        android:textSize="32dp" />

    <TextView
        android:id="@+id/textview_19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview_18"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="22dp"
        android:layout_marginRight="22dp"
        android:text="Solving difficult puzzles,Writing,Travelling"
        android:textColor="@color/white"
        android:textSize="30dp" />

    <TextView
        android:id="@+id/textview_20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview_19"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="15dp"
        android:layout_marginRight="22dp"
        android:background="@color/white"
        android:text="Achievements"
        android:textColor="@color/black"
        android:textSize="32dp" />

    <TextView
        android:id="@+id/textview_21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview_20"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="15dp"
        android:layout_marginRight="22dp"
        android:text="Certified Achievements:"
        android:textColor="@color/white"
        android:textSize="28dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textview_22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview_21"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="5dp"
        android:layout_marginRight="22dp"
        android:text="1.Second prize in writing Interschool Handwriting competition"
        android:textColor="@color/white"
        android:textSize="22dp" />

    <TextView
        android:id="@+id/textview_23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview_22"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="22dp"
        android:text="2.Fourth prize in College level Essay Competion"
        android:textColor="@color/white"
        android:textSize="22dp" />

    <TextView
        android:id="@+id/textview_24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview_23"
        android:layout_marginLeft="22dp"
        android:layout_marginTop="15dp"
        android:layout_marginRight="22dp"
        android:text="Uncertified Achievements:"
        android:textColor="@color/white"
        android:textSize="22dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textview_25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textview_24"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="11dp"
        android:layout_marginRight="22dp"
        android:text="Solved Rubix cube(Minimum time: 2min,20sec)"
        android:textColor="@color/white"
        android:textSize="22dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button_2"
        android:layout_below="@+id/textview_25"
        android:text="Back"
        android:layout_marginRight="22dp"
        android:layout_alignParentEnd="true"/>


</RelativeLayout>